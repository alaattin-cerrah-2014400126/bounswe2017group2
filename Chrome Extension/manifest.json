{
    "manifest_version": 2,
    "name": "L4C Annotation",
    "version": "0.1",
    "description": "A annotation application for websites",
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}